@Article{yousefpour_2009,
  Title                    = {Modelling of forest conversion planning with an adaptive simulation-optimization approach and simultaneous consideration of the values of timber, carbon and biodiversity},
  Author                   = {Yousefpour, Rasoul and Hanewinkel, Marc},
  Journal                  = {Ecological Economics},
  Year                     = {2009},

  Month                    = apr,
  Number                   = {6},
  Pages                    = {1711--1722},
  Volume                   = {68},

  Abstract                 = {The paper presents a combined simulationoptimization approach to model forest conversion planning taking the values of timber, carbon and biodiversity into account. The development of a virtual age-class forest was predicted by adopting a single tree forest growth tool, TreeGrOSS. The effects of different conversion regimes with continuous variables describing silvicultural prescriptions were monitored for a finite conversion period of forty years in stand level. Dynamic linear programming was employed to adaptively solve the multi-period large-scale optimization problem of a forest enterprise encompassing five different age-classes of pure Norway spruce (Picea abies, L. Karst) stands. The global net present values of biodiversity, carbon sequestration and timber production along with the wood even flow constraint, were considered simultaneously. The obtained optimal sylvicultural pathway differed not only among different stands but also sometimes within a given stand. The integration of the utility of biodiversity into the optimization procedure favoured conversion strategies that foresee the establishment of beech regeneration in all forest stands. The simultaneous consideration of all mentioned utilities resulted in a global utility of 27451 /ha consisting of 14499 (53\%), 3412 (12\%), 764 (3\%), and 8777 (32\%) /ha for the value of timber, carbon, biodiversity, and standing volume respectively. The sensitivity analysis showed that the threshold from which no major changes in the relevant silvicultural parameters occurred was observed at 30 /t carbon price with an interest rate of 2\%.},
  Doi                      = {10.1016/j.ecolecon.2008.12.009},
  File                     = {ScienceDirect Full Text PDF:/home/khusman1/.mozilla/firefox/ykffmvee.default/zotero/storage/RBB2KSUH/Yousefpour and Hanewinkel - 2009 - Modelling of forest conversion planning with an ad.pdf:application/pdf;ScienceDirect Snapshot:/home/khusman1/.mozilla/firefox/ykffmvee.default/zotero/storage/84MHV7VB/S0921800908005351.html:text/html},
  ISSN                     = {0921-8009},
  Keywords                 = {Biodiversity, Black Forest, Carbon sequestration, Dynamic linear programming, TreeGrOSS, Wood even flow},
  Owner                    = {khusman1},
  Series                   = {Eco-efficiency: {From} technical optimisation to reflective sustainability analysis},
  Timestamp                = {2017.04.16},
  Url                      = {http://www.sciencedirect.com/science/article/pii/S0921800908005351},
  Urldate                  = {2017-04-16}
}

