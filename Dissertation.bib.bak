@Article{hennig_2016,
  Title                    = {Sustainable feedstock potential - a limitation for the bio-based economy?},
  Author                   = {Hennig, Christiane and Brosowski, André and Majer, Stefan},
  Journal                  = {Journal of Cleaner Production},
  Year                     = {2016},

  Month                    = jun,
  Pages                    = {200--202},
  Volume                   = {123},

  Abstract                 = {Within the debate and research of the transition towards a bio-based economy a shortfall in considering and evaluating the actual available sustainable biomass potential for realizing this emerging concept can be noted. This paper provides insights into the role of feedstock availability and provision in particular addressing the current scientific discussion on the availability and cost provision of biomass resources in the context of energy production, and the implications for the technological concepts, mainly biorefineries. A lack of a harmonized methodological approach covering minimum requirements for determining biomass potentials can be seen. This does not allow a comparison and aggregation of the data for receiving a bigger picture on the feedstock potential. Consequently, academia and decision makers may not take a final position on the possible magnitude of an emerging bio-economy at the moment. The presented examples also reveal that a more distributed production might allow developing a larger share of the available biomass potential.},
  Doi                      = {10.1016/j.jclepro.2015.06.130},
  File                     = {ScienceDirect Full Text PDF:/home/khusman1/.mozilla/firefox/ykffmvee.default/zotero/storage/QX6D8E3E/Hennig et al. - 2016 - Sustainable feedstock potential  a limitation for.pdf:application/pdf;ScienceDirect Snapshot:/home/khusman1/.mozilla/firefox/ykffmvee.default/zotero/storage/4D6XGPXE/S095965261500863X.html:text/html},
  ISSN                     = {0959-6526},
  Keywords                 = {Bio-based economy, bioenergy, Biomass potential, Feedstock provision costs},
