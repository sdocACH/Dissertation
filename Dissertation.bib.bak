@Article{sanquetta_2015,
  Title                    = {Simultaneous estimation as alternative to independent modeling of tree biomass},
  Author                   = {Sanquetta, Carlos Roberto and Behling, Alexandre and Corte, Ana Paula Dalla and Netto, Sylvio Péllico and Schikowski, Ana Beatriz and Amaral, Mauricio Koubay do},
  Journal                  = {Annals of Forest Science},
  Year                     = {2015},

  Month                    = jul,
  Number                   = {8},
  Pages                    = {1099--1112},
  Volume                   = {72},

  Abstract                 = {Key message In this paper it is shown that a simultaneous adjustment provides more efficient estimates of total tree biomass than with independent modelling for biomass estimates by compartments (canopy, bole and roots). Context When modeling tree biomass, it is important to consider the additivity property, since the total tree biomass must be equal to the sum of the biomass of the components. Objective The aim of this study was to assess the simultaneous estimation performance, considering the additivity principle with respect to independent estimate when modeling biomass components and total biomass. Methods Individual modeling of total biomass and biomass components of leaves, branches, bole without bark, bole bark, and roots was performed on Pinus elliottii Engelm trees derived from forest stands in southern Brazil. Five nonlinear models were tested, and the best performance for estimating the total biomass of each component was selected, characterizing the independent estimation. The models selected for each component were fitted using the nonlinear seemingly unrelated regression method, which characterizes simultaneous estimation. Results Independent fitting of coefficients for biomass components and total biomass was not satisfactory, as the sum of the biomass component estimates diverged from the total biomass. This was not observed when the simultaneous fitting was used, which takes into account the additivity principle, and resulted in more effective estimators. Conclusion The simultaneous estimation method must be used in modeling tree biomass.},
  Doi                      = {10.1007/s13595-015-0497-2},
  File                     = {Full Text PDF:/home/khusman1/.mozilla/firefox/ykffmvee.default/zotero/storage/XPR3V7UX/Sanquetta et al. - 2015 - Simultaneous estimation as alternative to independ.pdf:application/pdf;Snapshot:/home/khusman1/.mozilla/firefox/ykffmvee.default/zotero/storage/6HC2HQCE/s13595-015-0497-2.html:text/html},
  ISSN                     = {1286-4560, 1297-966X},
  Keywords                 = {Additivity, Biomass components, Environment, general, Forestry, Forestry Management, Nonlinear seemingly unrelated regressions, Pinus elliottii, SAS, Tree Biology, Wood Science \& Technology},
  Language                 = {en},
  Owner                    = {khusman1},
  Timestamp                = {2017.03.02},
  Url                      = {http://link.springer.com.6681.han.sub.uni-goettingen.de/article/10.1007/s13595-015-0497-2},
  Urldate                  = {2016-02-03}
}

