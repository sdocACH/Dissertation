@Article{bockmann1998,
  author   = {B{\"o}ckmann, Th and Saborowski, J and Dahm, St and Nagel, J and Spellmann, H},
  title    = {A new conception for forest inventory in lower saxony},
  journal  = {Forst und Holz (Germany)},
  year     = {1998},
  abstract = {Strukturreiche, gemischte und ungleichaltrige Waldbestaende, geaenderte waldbauliche Zielsetzung und der angestrebte Umbau strukturarmer Waelder in strukturreiche Mischwaelder erschweren in manchen Forstaemtern Niedersachsens schon heute, zukuenftig in vielen Forstaemtern die Inventur und Planung im Rahmen der Betriebsregelung. In strukturreichen Waeldern stossen die herkoemmlichen, auf den Einzelbestand ausgerichtete Inventurverfahren an ihre Grenzen. Die notwendigen Bestockungsinformationen koennen nicht mehr mit einem angemessenen Aufwand bzw. mit einer vertretbaren Genauigkeit erhoben werden. In ungleichaltrigen Mischbestaenden verlieren die Ertragstafelmodelle ihre Aussagekraft fuer die Planung. Zudem muessen angesichts der angespannten Haushaltslage die Kosten der Forsteinrichtung gesenkt werden. - Aus diesen Gruenden wurde die bestehende Kontrollstichprobe in Niedersachsen weiterentwickelt. Es wird das neu konzipierte Stichprobenverfahren vorgestellt, das aufgrund seiner zweiphasigen Vorgehensweise die Zielgroesse "Vorrat" des wirtschaftlich und oekologisch wertvolleren Starkholzes der Hauptbaumarten mit einer hoeheren Genauigkeit einschaetzt als die des Schwachholzes. Es kann gezeigt werden, dass aufgrund der angestrebten deutlich hoeheren Schaetzgenauigkeiten in den oberen Durchmesserklassen mit dem zweiphasigen Verfahren die Stichprobenumfaenge gegenueber der klassischen Kontrollstichprobe reduziert werden koennen. Ferner wird der Messaufwand bei
den eigentlichenProbekreisaufnahmen durch Reduktion der Hoehenmessungen, Wegfall der D7-Messungen und Vereinfachung der gestaffelten Probekreise deutlich vermindert. Erste Erfahrungen mit der zweiphasigen Betriebsinventur einschliesslich der Kosten werden im Vergleich zur bisherigen Kontrollstichprobe am Beispiel des Forstamtes Erdmannshausen vorgestellt. Eine zusammenfassende Bewertung der Ergebnissse wird vorgenommen.},
  groups   = {Double sampling},
}

